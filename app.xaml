             xmlns:er="clr-namespace:PhotoReviewer"
             Startup="OnApplicationStartup">
    <Application.Resources>
        <ObjectDataProvider x:Key="Photos" ObjectType="{x:Type er:PhotoCollection}" />
    </Application.Resources>
</Application>